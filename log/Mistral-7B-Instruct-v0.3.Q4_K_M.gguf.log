import time

def timer(func):
    def wrapper(*args, **kwargs):
        start_time = time.time()
        result = func(*args, **kwargs)
        end_time = time.time()
        print(f"Time elapsed: {(end_time - start_time) * 1000} ms")
        return result
    return wrapper

@timer
def hello_world():
    for _ in range(5):
        print("Hello, world!")

hello_world()
